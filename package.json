{
  "name": "react-webpacker",
  "version": "1.5.2",
  "description": "The smallest, enoughly-tested library for react with webpacker.",
  "main": "dist/react-webpacker.js",
  "module": "src/react-webpacker.js",
  "types": "src/react-webpacker.d.ts",
  "scripts": {
    "build": "babel src --source-maps --out-dir dist",
    "fmt": "prettier --write \"{src,test}/**/*.js{,x}\"",
    "lint": "eslint \"{src,test}/**/*.js{,x}\" && tslint \"src/**/*.ts{,x}\"",
    "prepare": "npm run build",
    "test": "jest test/react-webpacker.test.js"
  },
  "author": "euxn23",
  "license": "MIT",
  "keywords": [
    "react",
    "webpack",
    "webpacker",
    "ruby",
    "rails"
  ],
  "devDependencies": {
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.17.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.12.1",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.2",
    "jest": "^22.2.2",
    "prettier": "^1.10.2",
    "prettier-cli": "^0.1.0",
    "puppeteer": "^1.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.9.2",
    "webpack": "^3.11.0"
  }
}
